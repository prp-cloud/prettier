{
  "name": "prettier",
  "version": "3.4.0-894b9ec7",
  "description": "Prettier is an opinionated code formatter",
  "bin": "./dist/bin/prettier.cjs",
  "repository": "prettier/prettier",
  "funding": "https://github.com/prettier/prettier?sponsor=1",
  "homepage": "https://prettier.io",
  "author": "James Long",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "browser": "./standalone.js",
  "unpkg": "./standalone.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@angular/compiler": "18.0.2",
    "@babel/code-frame": "7.24.7",
    "@babel/generator": "7.24.7",
    "@babel/parser": "7.24.7",
    "@babel/types": "7.24.7",
    "@eslint/compat": "1.0.3",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.5.0",
    "@glimmer/reference": "0.92.0",
    "@glimmer/syntax": "0.92.0",
    "@iarna/toml": "2.2.5",
    "@prettier/is-es5-identifier-name": "0.2.0",
    "@prettier/parse-srcset": "3.1.0",
    "@stylistic/eslint-plugin-js": "2.2.2",
    "@types/eslint__js": "8.42.3",
    "@types/estree": "1.0.5",
    "@types/file-entry-cache": "5.0.4",
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "8.0.0-alpha.34",
    "@typescript-eslint/typescript-estree": "8.0.0-alpha.34",
    "@typescript-eslint/visitor-keys": "8.0.0-alpha.34",
    "acorn": "8.12.0",
    "acorn-jsx": "5.3.2",
    "angular-estree-parser": "10.0.3",
    "angular-html-parser": "5.2.0",
    "benchmark": "2.1.4",
    "browserslist": "4.23.1",
    "browserslist-to-esbuild": "2.1.1",
    "c8": "10.1.2",
    "camelcase": "8.0.0",
    "chalk": "5.3.0",
    "ci-info": "4.0.0",
    "cjk-regex": "3.1.0",
    "collapse-white-space": "1.0.6",
    "cross-env": "7.0.3",
    "cspell": "8.8.4",
    "css-units-list": "2.0.1",
    "dashify": "2.0.0",
    "diff": "5.2.0",
    "editorconfig": "0.15.3",
    "emoji-regex": "10.3.0",
    "enquirer": "2.4.1",
    "esbuild": "0.22.0",
    "esbuild-plugins-node-modules-polyfill": "1.6.4",
    "esbuild-visualizer": "0.6.0",
    "escape-string-regexp": "5.0.0",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-formatter-friendly": "7.0.0",
    "eslint-plugin-compat": "5.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-n": "17.9.0",
    "eslint-plugin-react": "7.34.3",
    "eslint-plugin-regexp": "2.6.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unicorn": "54.0.0",
    "esm-utils": "4.3.0",
    "espree": "10.1.0",
    "execa": "7.2.0",
    "fast-glob": "3.3.2",
    "fast-json-stable-stringify": "2.1.0",
    "file-entry-cache": "9.0.0",
    "find-cache-dir": "5.0.0",
    "flow-parser": "0.237.2",
    "get-east-asian-width": "1.2.0",
    "get-stdin": "9.0.0",
    "graphql": "16.9.0",
    "hermes-parser": "0.22.0",
    "html-element-attributes": "3.4.0",
    "html-tag-names": "2.1.0",
    "html-ua-styles": "0.0.6",
    "ignore": "5.3.1",
    "import-meta-resolve": "4.1.0",
    "iterate-directory-up": "1.1.1",
    "jest": "30.0.0-alpha.5",
    "jest-docblock": "30.0.0-alpha.5",
    "jest-light-runner": "0.6.0",
    "jest-snapshot-serializer-ansi": "2.1.0",
    "jest-snapshot-serializer-raw": "2.0.0",
    "jest-watch-typeahead": "2.2.2",
    "js-yaml": "4.1.0",
    "json5": "2.2.3",
    "leven": "4.0.0",
    "lines-and-columns": "2.0.4",
    "linguist-languages": "7.27.0",
    "magic-string": "0.30.10",
    "meriyah": "4.5.0",
    "micromatch": "4.0.7",
    "minimist": "1.2.8",
    "n-readlines": "1.0.1",
    "npm-run-all2": "6.2.0",
    "outdent": "0.8.0",
    "parse-json": "8.1.0",
    "postcss": "8.4.39",
    "postcss-less": "6.0.0",
    "postcss-media-query-parser": "0.2.3",
    "postcss-scss": "4.0.9",
    "postcss-selector-parser": "2.2.3",
    "postcss-values-parser": "2.0.1",
    "prettier": "3.3.2",
    "pretty-bytes": "6.1.1",
    "remark-footnotes": "2.0.0",
    "remark-math": "3.0.1",
    "remark-parse": "8.0.3",
    "rollup-plugin-license": "3.4.1",
    "sdbm": "2.0.0",
    "semver": "7.6.2",
    "serialize-javascript": "6.0.2",
    "snapshot-diff": "0.10.0",
    "strip-ansi": "7.1.0",
    "tempy": "3.1.0",
    "to-fast-properties": "4.0.0",
    "ts-expect": "1.3.0",
    "typescript": "5.5.3",
    "unicode-regex": "4.0.0",
    "unified": "9.2.2",
    "url-or-path": "2.3.0",
    "vnopts": "2.0.0",
    "wcwidth.js": "1.1.2",
    "yaml-unist-parser": "2.0.1"
  },
  "resolutions": {
    "trim": "1.0.1"
  },
  "scripts": {
    "prepublishOnly": "echo \"Error: must publish from dist/\" && exit 1",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:dev-package": "cross-env INSTALL_PACKAGE=1 yarn test",
    "test:dist": "cross-env NODE_ENV=production yarn test",
    "test:dist-standalone": "cross-env TEST_STANDALONE=1 yarn test:dist",
    "test:dist-lint": "eslint dist --config=./scripts/bundle-eslint-config.js --quiet",
    "perf": "yarn && yarn build && cross-env NODE_ENV=production node ./dist/bin/prettier.cjs",
    "perf:inspect": "yarn && yarn build && cross-env NODE_ENV=production node --inspect-brk ./dist/bin/prettier.cjs",
    "perf:benchmark": "yarn perf --debug-benchmark",
    "perf:compare": "./scripts/benchmark/compare.sh",
    "lint": "run-p \"lint:*\"",
    "lint:typecheck": "tsc",
    "lint:eslint": "cross-env EFF_NO_LINK_RULES=true eslint . --format friendly",
    "lint:changelog": "node ./scripts/lint-changelog.js",
    "lint:prettier": "prettier . --check --cache",
    "lint:spellcheck": "cspell --no-progress --relative --dot --gitignore",
    "lint:deps": "node ./scripts/check-deps.js",
    "lint:blog": "node ./scripts/lint-blog.js",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "build": "node ./scripts/build/build.js",
    "build:website": "node ./scripts/build-website.js",
    "gen:changelog": "node ./scripts/generate-changelog.js",
    "debug": "node bin/prettier.js --ignore-path=.prettierignore"
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "include": [
      "src/**",
      "bin/**"
    ],
    "exclude": [
      "bin/prettier.js",
      "src/standalone.js",
      "src/index.cjs",
      "src/document/debug.js",
      "src/utils/unexpected-node-error.js",
      "src/language-js/types/estree.d.ts",
      "src/**/**/*.d.ts"
    ]
  },
  "browserslist": [
    ">0.5%",
    "not dead",
    "not op_mini all"
  ],
  "preferUnplugged": true,
  "packageManager": "yarn@4.3.1",
  "overrides": {
    "@typescript-eslint/eslint-plugin": "$@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser": "$@typescript-eslint/eslint-plugin",
    "esbuild": "$esbuild",
    "eslint": "$eslint",
    "jest": "$jest"
  }
}
